{
  "name": "todo",
  "version": "1.0.0",
  "description": "An online todo management tool.",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "tslint": "tslint \"*.ts\"",
    "rev-static": "rev-static --config rev-static.config.js",
    "webpack": "webpack --config webpack.config.js",
    "lessc": "lessc index.less > index.css",
    "stylelint": "stylelint \"**/*.less\"",
    "cleancss": "cleancss -o index.min.css index.css  ./node_modules/github-fork-ribbon-css/gh-fork-ribbon.css",
    "clean": "rimraf index.min-*.js index.min-*.css",
    "file2variable": "file2variable-cli index.template.html -o variables.ts --html-minify",
    "build": "npm run clean && npm run file2variable && npm run tsc && npm run lessc && npm run cleancss && npm run webpack && npm run rev-static && npm run sw-precache",
    "lint": "npm run tslint && npm run stylelint && npm run standard",
    "sw-precache": "sw-precache --config sw-precache.config.js --verbose && uglifyjs service-worker.js -o service-worker.bundle.js",
    "standard": "standard \"**/*.config.js\"",
    "fix": "standard --fix \"**/*.config.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/todo.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/todo/issues"
  },
  "homepage": "https://github.com/plantain-00/todo#readme",
  "devDependencies": {
    "@types/clipboard": "1.5.34",
    "@types/json5": "0.0.29",
    "clean-css-cli": "4.1.6",
    "clipboard": "1.7.1",
    "file2variable-cli": "1.3.0",
    "github-fork-ribbon-css": "0.2.1",
    "json5": "0.5.1",
    "less": "2.7.2",
    "relative-time-component": "1.1.0",
    "rev-static": "2.5.0",
    "rimraf": "2.6.1",
    "standard": "10.0.2",
    "stylelint": "7.12.0",
    "stylelint-config-standard": "16.0.0",
    "sw-precache": "5.2.0",
    "tslib": "1.7.1",
    "tslint": "5.4.3",
    "typescript": "2.4.1",
    "uglify-js": "3.0.22",
    "vue": "2.3.4",
    "vue-class-component": "5.0.2",
    "webpack": "3.0.0"
  }
}
