{
  "name": "todo",
  "version": "1.0.0",
  "description": "An online todo management tool.",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "tslint": "tslint \"*.ts\"",
    "rev-static": "rev-static --config rev-static.config.js",
    "webpack": "webpack --config webpack.config.js",
    "lessc": "lessc index.less > index.css",
    "stylelint": "stylelint \"**/*.less\"",
    "cleancss": "cleancss -o index.min.css index.css",
    "clean": "rimraf index-*.*",
    "file2variable": "file2variable-cli index.template.html -o variables.ts --html-minify",
    "build": "npm run clean && npm run file2variable && npm run tsc && npm run lessc && npm run cleancss && npm run webpack && npm run rev-static && npm run service-worker",
    "lint": "npm run tslint && npm run stylelint",
    "service-worker": "webpack --config webpack.config.worker.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/todo.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/todo/issues"
  },
  "homepage": "https://github.com/plantain-00/todo#readme",
  "devDependencies": {
    "clean-css-cli": "4.1.3",
    "file2variable-cli": "1.3.0",
    "less": "2.7.2",
    "rev-static": "2.4.2",
    "rimraf": "2.6.1",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "tslib": "1.7.1",
    "tslint": "5.2.0",
    "typescript": "2.3.2",
    "vue": "2.3.3",
    "vue-class-component": "5.0.1",
    "webpack": "2.5.1"
  }
}
